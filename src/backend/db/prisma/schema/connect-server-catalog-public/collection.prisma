model ServerListingCollection {
  oid BigInt @id @default(autoincrement())
  id  String @unique

  name        String
  slug        String @unique
  description String

  listings ServerListing[]

  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}

model ServerListingCategory {
  oid BigInt @id @default(autoincrement())
  id  String @unique

  name        String
  slug        String @unique
  description String

  listings ServerListing[]

  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}
