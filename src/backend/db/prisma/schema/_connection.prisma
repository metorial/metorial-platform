datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider   = "prisma-client-js"
  // binaryTargets = ["native", "debian-openssl-1.1.x", "debian-openssl-3.0.x"]
  output     = "../generated"
  engineType = "client"
}

generator json {
  provider = "prisma-json-types-generator"
}
