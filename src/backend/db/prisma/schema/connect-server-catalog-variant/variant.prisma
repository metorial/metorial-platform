model ServerVariant {
  oid BigInt @id @default(autoincrement())
  id  String @unique

  providerOid BigInt
  provider    ServerVariantProvider @relation(fields: [providerOid], references: [oid])

  serverOid BigInt
  server    Server @relation(fields: [serverOid], references: [oid])

  currentConfigOid BigInt?
  currentConfig    ServerConfig? @relation(fields: [currentConfigOid], references: [oid], name: "ServerVariantCurrentServerConfig")

  versions ServerVersion[]
  configs  ServerConfig[]
}
